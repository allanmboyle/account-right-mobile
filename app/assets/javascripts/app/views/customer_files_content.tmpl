<div id="customer-files-list" data-role="collapsible-set" data-theme="b" data-content-theme="a">
    <% customerFiles.each(function(customerFile) { %>
        <div class="customer-file" data-role="collapsible" data-theme="b">
            <h3>
                <div class="customer-file-name"><%= customerFile.get('Name') %></div>
                <div style="font: -9"> Small text goes under here</div>
            </h3>
        </div>
    <% }); %>
    <div class="no-files myob-hidden myob-message">Note: You do not have access to any client files at this time.</div>
</div>
